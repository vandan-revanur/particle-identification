! Initialize Pythia

Beams:eCM = 13000.  ! center-of-mass energy
HardQCD:all = on    ! turn on hard QCD processes
Random:seed = 0     ! set random seed to 0

! Define the detector geometry
! set radius and length of cylindrical detector in meters

ParticleDecays:Rmax = 2.0
ParticleDecays:Zmax = 5.0


! End of configuration
